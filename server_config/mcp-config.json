{
  "mcpServers": {
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/ajlennon/mcp-service/files"
      ]
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "openscad": {
      "command": "/home/ajlennon/stable-diffusion-webui/venv/bin/python",
      "args": ["/home/ajlennon/mcp-service/create_simple_openscad_server.py"],
      "env": {
        "VIRTUAL_ENV": "/home/ajlennon/stable-diffusion-webui/venv"
      }
    }
  }
}